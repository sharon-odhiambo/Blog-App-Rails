    </div>
    <div>
    <p>Comments: <%= post.comments_counter %></p>
    <p>Likes: <%= post.likes_counter %></p>
    </div>
<% end %>
<p><%= link_to user_posts_path(@user) do %></p>
<p>See all Posts<% @user.posts %></p>
<% end %>